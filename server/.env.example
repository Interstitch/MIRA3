# MIRA Server Environment Variables
# Copy this to .env and fill in values

# Server IP - the IP address clients will connect to
# Use your LAN IP, Tailscale IP, or 127.0.0.1 for localhost only
TAILSCALE_IP=192.168.1.100

# PostgreSQL - set a strong password
POSTGRES_PASSWORD=your_secure_password_here

# Optional: Override defaults
# POSTGRES_DB=mira
# POSTGRES_USER=mira
